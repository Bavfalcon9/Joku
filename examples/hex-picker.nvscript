// This example uses linear regression
@systems(for=linear_regression)

use math
use ml.util

enum SHADE {
    DARK,
    LIGHT
}

initial_set: util.TrainingSet<SHADE>[] = [
    {
        in: 0xFF,
        output: SHADE.LIGHT
    },
    {
        in: 0x00,
        output: SHADE.DARK
    }
]

network = util.nn_network(math.LINEAR)
network.train(initial_set, )